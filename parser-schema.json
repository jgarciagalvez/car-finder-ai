{
  "sites": {
    "otomoto": {
      "method": "json",
      "scriptSelector": "script#__NEXT_DATA__",
      "autoDetection": {
        "searchPageIndicator": "props.pageProps.urqlState",
        "detailPageIndicator": "props.pageProps.advert"
      },
      "pageTypes": {
        "search": {
          "basePath": "props.pageProps.urqlState",
          "dataPath": "data",
          "listPath": "advertSearch.edges",
          "fields": {
            "sourceId": "node.id",
            "sourceUrl": "node.url", 
            "sourceTitle": "node.title",
            "sourceCreatedAt": "node.createdAt"
          }
        },
        "detail": {
          "basePath": "props.pageProps.advert",
          "fields": {
            "sourceId": "id",
            "sourceTitle": "title",
            "sourceUrl": "url",
            "pricePln": "price.value",
            "priceCurrency": "price.currency",
            "sourceDescriptionHtml": "description",
            "sourceCreatedAt": "createdAt",
            "sellerName": "seller.name",
            "sellerId": "seller.id",
            "sellerType": "seller.type",
            "sellerLocation": "seller.location.address",
            "memberSince": "seller.featuresBadges[code=registration-date].label",
            "year": "details[label=Rok produkcji].value",
            "mileage": "details[label=Przebieg].value",
            "sourceParameters": "details",
            "sourceEquipment": "equipment",
            "sourcePhotos": "images.photos"
          }
        }
      }
    },
    "olx": {
      "method": "json",
      "scriptSelector": "script",
      "dataExtraction": "window.__PRERENDERED_STATE__",
      "autoDetection": {
        "searchPageIndicator": "listing.ads",
        "detailPageIndicator": "ad.ad"
      },
      "pageTypes": {
        "search": {
          "basePath": "listing.ads",
          "listPath": "",
          "fields": {
            "sourceId": "id",
            "sourceUrl": "url",
            "sourceTitle": "title",
            "sourceDescriptionHtml": "description",
            "sourceCreatedAt": "createdTime",
            "pricePln": "price.regularPrice.value",
            "priceCurrency": "price.regularPrice.currencyCode",
            "sellerName": "user.name",
            "sellerId": "user.id",
            "sellerLocation": "location.cityName",
            "sellerRegion": "location.regionName",
            "memberSince": "user.created",
            "sourcePhotos": "photos",
            "sourceParameters": "params"
          },
          "parameterMapping": {
            "year": "rok-produkcji",
            "mileage": "przebieg-pojazdu",
            "fuelType": "rodzaj-paliwa",
            "enginePower": "moc-silnika",
            "engineSize": "pojemnosc-skokowa",
            "transmission": "skrzynia-biegow",
            "color": "kolor",
            "condition": "stan"
          }
        },
        "detail": {
          "basePath": "ad.ad",
          "fields": {
            "sourceId": "id",
            "sourceTitle": "title",
            "sourceUrl": "url",
            "sourceDescriptionHtml": "description",
            "sourceCreatedAt": "created_time",
            "pricePln": "price.value.raw",
            "priceCurrency": "price.value.currency",
            "sellerName": "user.name",
            "sellerId": "user.id",
            "sellerLocation": "location.city.name",
            "sourcePhotos": "photos"
          }
        }
      }
    }
  }
}
